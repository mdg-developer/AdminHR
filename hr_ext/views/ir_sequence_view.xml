<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_pocket_expense" model="ir.sequence">
		    <field name="name">Pocket Expense</field>
			<field name="code">hr.pocket.expense</field>
			<field name="prefix">Exp/%(range_year)s/</field>
			<field eval="4" name="padding"/>
			<field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
		</record>
		
        <record id="sequence_travel_expense" model="ir.sequence">
		    <field name="name">Travel Expense</field>
			<field name="code">hr.travel.expense</field>
			<field name="prefix">Exp/%(range_year)s/</field>
			<field eval="4" name="padding"/>
			<field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
		</record>

		<record id="sequence_trip_expense" model="ir.sequence">
		    <field name="name">Trip Expense</field>
			<field name="code">admin.trip.expense</field>
			<field name="prefix">Exp/T/%(range_year)s/</field>
			<field eval="4" name="padding"/>
			<field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
		</record>
		
    </data>
</odoo>
