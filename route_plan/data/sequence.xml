<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="route_sequence" model="ir.sequence">			
		    <field name="name">Route Code</field>			
			<field name="code">route.code</field>
			<field name="prefix">RT/</field>
			<field eval="6" name="padding"/>
			<field name="company_id" eval="False" />
		</record>
		
		<record id="plan_trip_waybill_sequence" model="ir.sequence">
		    <field name="name">Plan Trip Waybill Code</field>
			<field name="code">plan.trip.waybill.code</field>
			<field name="prefix">PTW/%(year)s/</field>
			<field eval="6" name="padding"/>
			<field name="company_id" eval="False" />
		</record>
		
		<record id="plan_trip_product_sequence" model="ir.sequence">
		    <field name="name">Plan Trip Product Code</field>
			<field name="code">plan.trip.product.code</field>
			<field name="prefix">PTP/%(year)s/</field>
			<field eval="6" name="padding"/>
			<field name="company_id" eval="False" />
		</record>

		<record id="day_plan_trip_sequence" model="ir.sequence">
		    <field name="name">Day Plan Trip Code</field>
			<field name="code">day.plan.trip.code</field>
			<field name="prefix">DT/%(year)s/</field>
			<field eval="6" name="padding"/>
			<field name="company_id" eval="False" />
		</record>
	</data>
</odoo>